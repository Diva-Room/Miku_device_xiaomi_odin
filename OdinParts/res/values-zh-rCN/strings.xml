<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- DC dimming -->
    <string name="dc_dimming_title">DC 调光</string>
    <string name="dc_dimming_summary">可以减轻使用手机时的眼部疲劳，可能会降低显示质量</string>
    <string name="dc_dimming_failed">DC 调光开启失败！</string>

     <!-- Fast charge -->
    <string name="fast_charge_title">快速充电</string>
    <string name="fast_charge_summary">可以极大提高充电速度，但是可能会造成发热</string>

     <!-- High touch polling rate -->
    <string name="touch_polling_rate_title">高触控采样率</string>
    <string name="touch_polling_rate_summary">将触控刷新率从240hz提升到480hz，提高触控跟手度，可能会略微增加耗电</string>

    <!-- Wireless reverse charging -->
    <string name="power_share_title">无线反向充电</string>
    <string name="power_share_summary">为其它设备进行无线充电</string>

    <!-- HBM -->
    <string name="hbm_title">阳光模式</string>
    <string name="hbm_summary">使屏幕发出更高亮度，在太阳底下看的更清楚，开启后可能会导致屏下摄像头CUP区域显示异常</string>
    <string name="hbm_failed">阳光模式开启失败！</string>

</resources>
